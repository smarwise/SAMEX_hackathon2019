<html>
    <head>
        <title>Progress</title>
    </head>
    <div class="container">
        <b>Points:</b> {{points}}<br />
        <b>Events attended: </b>{{events}}
    </div>
    <div class="container"> Skills Developed
        {{!-- {{#if percent}} --}}
            {{#each skills}}
                {{#each ../percent}}
                <li> {{../this}}</li>
                <div class="progress">
                    <div class="progress-bar" role="progressbar" aria-valuenow="70"
                    aria-valuemin="0" aria-valuemax="100" style="width:{{this}}%">
                        <span class="sr-only">70% Complete</span>
                    </div>
                    </div>
                {{/each}}
            {{/each}}
        {{!-- {{/if}} --}}
    </div>
</html>